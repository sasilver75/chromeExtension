{
  "manifest_version": 2,
  "name": "Chippy",
  "version": "1.0",
  "description": "Spaced Repetition App",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.3.1.min.js", "content.js"]
  }],
  "browser_action": {
    "default_icon": "brain_orange.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}